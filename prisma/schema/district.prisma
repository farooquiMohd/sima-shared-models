model District {
    id     Int    @id @default(autoincrement())
    name   String
    status Status @default(ACTIVE)

    stateId Int
    state   State @relation(fields: [stateId], references: [id])

    tehsils Tehsil[]

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    mpps      MPP[]
    mccs      MCC[]

    @@unique([name, stateId])
    @@map("districts")
}
