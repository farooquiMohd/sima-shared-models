model Customer {
    id          Int     @id @default(autoincrement())
    companyName String
    email       String  @unique
    phoneNumber String
    status      Status  @default(ACTIVE)
    industry    String?
    address     String?

    tickets Ticket[]

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("customers")
}
