model SubCategory {
    id          Int     @id @default(autoincrement())
    name        String
    description String?
    fileId      Int?

    categoryId Int
    category   Category @relation(fields: [categoryId], references: [id])

    file File? @relation(fields: [fileId], references: [id])

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    assets Asset[]
}
