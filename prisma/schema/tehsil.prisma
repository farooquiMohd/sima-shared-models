model Tehsil {
    id     Int    @id @default(autoincrement())
    name   String
    status Status @default(ACTIVE)

    districtId Int
    district   District @relation(fields: [districtId], references: [id])

    villages Village[]

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    mpps      MPP[]
    mccs      MCC[]

    @@unique([name, districtId])
}
